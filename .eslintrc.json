{
    "extends": "eslint:recommended",
    "rules": {
      "no-unused-vars": 2,
      "no-undef": 2,
      "no-restricted-imports": "warn"
    },
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "env": {
      "es6": true,
      "browser": true,
      "node": true,
      "jest": true
    }
  }